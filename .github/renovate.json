{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:recommended"
	],
	"addLabels": [
		"dependencies"
	],
	"automergeType": "branch",
	"packageRules": [
		{
			"matchUpdateTypes": [
				"minor",
				"patch",
				"pin",
				"digest"
			],
			"automerge": true
		}
	]
}
